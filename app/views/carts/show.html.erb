<h1>ここはカートのページです</h1>
<table>
	<tr><th>商品名</th><th>値段</th></tr>
	<% @cart_products.each do |cart_product| %>
		<tr>
	  		<th><%= cart_product.product.product_name %></th>
	  		<th><%= cart_product.product.price %></th>
	  		<th><%= link_to 'カートから削除する', destroy_cart_product_path(product_id: cart_product.product_id), method: :delete %></th>
		</tr>
	<% end %>
</table>